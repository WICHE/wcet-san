{
    "name": "e3/paragon",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "drupal/core-dev": "^9.5",
        "drupal/upgrade_rector": "^2.0@alpha"
    },
    "require": {
        "composer/installers": "^1.0",
        "drupal/core-composer-scaffold": "^9.4.9",
        "drupal/core-recommended": "^9.4.9",
        "cweagans/composer-patches": "~1.0",
        "wikimedia/composer-merge-plugin": "dev-master",
        "drupal/core": "^9.4.9",
        "drush/drush": "^11",
        "drupal/coffee": "^1.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/adminimal_theme": "^1.0",
        "drupal/adminimal_admin_toolbar": "^1.1",
        "drupal/config_update": "^2.0",
        "drupal/contact_storage": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/metatag": "^1.19",
        "drupal/redirect": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_previewer": "^1.2",
        "drupal/inline_entity_form": "^1.0",
        "drupal/menu_block": "^1.0",
        "drupal/smart_trim": "^2.0",
        "drupal/link_attributes": "^1.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/entity_browser": "^2.0",
        "drupal/focal_point": "^2.0",
        "drupal/embed": "^1.0",
        "drupal/entity_embed": "^1.0",
        "drupal/address": "^1.11",
        "drupal/name": "^1.0",
        "drupal/search_api": "^1.0",
        "drupal/search_api_solr": "^4.2",
        "drupal/chosen": "^3.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_tools": "dev-1.x",
        "drupal/google_analytics": "^4.0",
        "drupal/ultimate_cron": "^2.0",
        "drupal/login_security": "^2.0",
        "drupal/markup": "^2.0",
        "drupal/system_status": "^2.0",
        "drupal/paragraphs_browser": "^1.0-alpha2",
        "drupal/twig_field_value": "^2.0",
        "drupal/linkit": "^6.0",
        "drupal/field_hidden": "^2.0",
        "drupal/mailsystem": "^4.0",
        "drupal/image_style_quality": "^1.0",
        "drupal/sitemap": "^2.0",
        "drupal/contact_storage_export": "^1.0",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^2.0",
        "drupal/advagg": "^6.0@alpha",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/allowed_formats": "^2.0",
        "drupal/swiftmailer": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/scheduler": "^1.0",
        "drupal/entity_clone": "^2.0@beta",
        "symfony/css-selector": "~4.4",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/block_field": "^1.0",
        "drupal/migrate_source_csv": "^3.0",
        "drupal/simple_sitemap": "^4.0",
        "drupal/token": "^1.7",
        "drupal/graphql": "^3.0",
        "drupal/graphql_search_api": "^1.1",
        "drupal/search_api_exclude_entity": "^1.0",
        "drupal/media_entity_generic": "^1.1",
        "drupal/csv_serialization": "^2.0 || ^3.0",
        "twig/twig": "^2.12.0",
        "drupal/upgrade_status": "^4.0",
        "drupal/webform": "^6.2",
        "drupal/views_data_export": "^1.1",
        "drupal/webform_views": "^5.0@beta",
        "drupal/entity_print": "^2.5",
        "drupal/config_ignore": "^3.0",
        "drupal/config_filter": "^2.4",
        "palantirnet/drupal-rector": "^0.15.1",
        "drupal/rdf": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/ParagonScriptHandler.php"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "excludes": [
                ".htaccess",
                "robots_hidden.txt",
                "docroot/modules/contrib/field_tools/.git",
                "docroot/modules/contrib/security_review/.git"
            ]
        },
        "installer-paths": {
            "docroot/core": [
                "drupal/core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "merge-plugin": {
            "include": [
                "docroot/modules/custom/*/composer.json"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/field_hidden": {
                "d10_compatibility": "https://www.drupal.org/files/issues/2023-05-11/d10_compatibility-3287517-3.patch"
            },
            "drupal/csv_serialization": {
                "d10_compatibility": "https://www.drupal.org/files/issues/2022-11-03/csv_serialization-3295036-5-d10_compat.patch"
            },
            "drupal/system_status": {
                "d10_compatibility": "https://www.drupal.org/files/issues/2023-02-15/system_status-drupal_10_compatibility-3299281-7.patch"
            },
            "drupal/adminimal_admin_toolbar": {
                "d10_compatibility": "https://www.drupal.org/files/issues/2022-06-15/adminimal_admin_toolbar.1.x-dev.rector.patch"
            },
            "drupal/graphql_search_api": {
                "d10_compatibility": "./patches/graphql_search_api_d10_compatibility.patch"
            }
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ParagonScriptHandler::createPrivateTempDirectories",
            "DrupalProject\\composer\\ParagonScriptHandler::makeRobotsHidden"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ParagonScriptHandler::createPrivateTempDirectories",
            "DrupalProject\\composer\\ParagonScriptHandler::makeRobotsHidden"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal-composer/drupal-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/core-composer-scaffold": true
        }
    }
}
