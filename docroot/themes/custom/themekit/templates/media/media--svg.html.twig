{#
/**
 * @file
 * Default theme implementation to present a media entity.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes }}>
  {% if svg_url %}
    <div class="svg-container">
      <img class="inject-me" src="{{ svg_url }}">
    </div>
  {% endif %}
</div>
